RewriteEngine On
Options -MultiViews
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^(index|contact_me|edit_profile|login|signup|about_me|projects|blogs|request_password_change|change_password)$ $1.php [NC,L]
RewriteRule ^blog/(\d+)$ blog.php?blog_id=$1 [NC,L]
RewriteRule ^project/(\d+)$ project.php?project_id=$1 [NC,L]